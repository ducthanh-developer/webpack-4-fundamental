  "description": "This project for learn webpack 4 ",
  "main": "index.js",
  "scripts": {
    "webpack": "webpack --env mode=development",
    "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js",
    "dev": "webpack serve --env mode=development --open --hot",
    "prod": "webpack --env mode=production",
    "prod:typescript": "npm run prod --env presets=typescript",
    "prod:analyze": "npm run prod --env presets=analyze",
    "prod:compress":"npm run prod --env presets=compress",
    "dev:debug": "npm run debug --env mode=development",
    "prod:debug": "npm run debug --env mode=production"
